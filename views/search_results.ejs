<!-- search_results.ejs -->
<!DOCTYPE html>
<html lang="en">

<head>
    <meta charset="UTF-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>Search Results</title>
</head>

<body>
    <h1>Search Results</h1>
    <ul>
        <% movies.forEach(movie=> { %>
            <li>
                <%= movie.title %>
            </li> <!-- Display movie title -->
            <% }); %>
    </ul>
</body>

</html>